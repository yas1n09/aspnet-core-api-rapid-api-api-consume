@model List<ResultRoomDto>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
    int count = 0;
}
<table class="table table-bordered verticle-middle">
    <tr>
        <th>#</th>
        <th>Oda Numarası</th>
        <th>Başlık</th>
        <th>Güncelle - Sil</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <th>@count</th>
            <td>@item.RoomNumber</td>
            <td>@item.Title</td>
            <td>
                <a href="/AdminRoom/UpdateRoom/@item.RoomID" class="fa fa-pencil color-muted m-r-5" style="margin-left:10px;"></a>
                <a href="/AdminRoom/DeleteRoom/@item.RoomID" class="fa fa-close color-danger" style="margin-left:50px;"></a>
            </td>

        </tr>
    }
</table>
<a href="/AdminRoom/AddRoom/" class="btn mb-1 btn-outline-primary">Yeni Oda Girişi</a>